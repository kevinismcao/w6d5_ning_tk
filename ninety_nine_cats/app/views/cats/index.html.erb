<h1>Our cats!</h1>

<ul>
<% @cats.each do |cat| %>
    <br>
    <li>Name: <%= cat.name%></li>
    <li>Birthday: <%= cat.birth_date%></li>
    <li>Color: <%= cat.color%></li>
    <li>Sex: <%= cat.sex %></li>
    <li>Description: <%= cat.description %></li>
    <a href="<%=cat_url(cat)%>"> Cat's Show Page</a>
    <br>
<% end %>
</ul>