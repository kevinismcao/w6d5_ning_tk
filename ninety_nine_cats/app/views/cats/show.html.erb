<h1>Cat Show Page</h1>

<table>
    <thead>
        <tr>
            <th colspan="2">The cat</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>Name:</td>
            <td><%= @cat.name%></td>
        </tr>
         <tr>
            <td>Color:</td>
            <td><%= @cat.color%></td>
        </tr>
        <tr>
            <td>Birthday:</td>
            <td><%= @cat.birth_date%></td>
        </tr>
        <tr>
            <td>Sex:</td>
            <td><%= @cat.sex%></td>
        </tr>
        <tr>
            <td colspan="2">Description:<%= @cat.description%></td>
            
        </tr>
    </tbody>
</table>
